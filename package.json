{
  "name": "nucleale-app",
  "version": "1.0.0",
  "description": "Official Nucleale App",
  "main": "index.js",
  "scripts": {
    "test": "TZ=UTC jest --passWithNoTests --noStackTrace --no-cache --runInBand",
    "test:unit": "npm test -- --watch --config jest-unit.config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "prepare": "husky install",
    "dev": "webpack serve --config webpack.dev.js --open",
    "storybook": "start-storybook",
    "build:storybook": "build-storybook -c .storybook -o sb",
    "build": "webpack --config webpack.prod.js",
    "start": "node index"
  },
